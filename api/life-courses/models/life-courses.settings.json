{
  "kind": "collectionType",
  "collectionName": "life_courses",
  "info": {
    "name": "lifeCourses",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "slug": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "article": {
      "type": "dynamiczone",
      "components": [
        "live-corses.block",
        "live-corses.points"
      ]
    },
    "picture": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "date": {
      "type": "date"
    },
    "previewOnly": {
      "type": "boolean",
      "default": true
    },
    "courseOpened": {
      "type": "boolean",
      "default": true
    },
    "studyField": {
      "type": "string"
    },
    "studyFieldSlug": {
      "type": "string"
    },
    "seo": {
      "type": "component",
      "repeatable": true,
      "component": "shared.seo"
    },
    "duration": {
      "type": "integer"
    },
    "price": {
      "type": "integer"
    },
    "slugDescription": {
      "type": "component",
      "repeatable": true,
      "component": "live-corses.slug-description"
    },
    "isPopular": {
      "type": "boolean",
      "default": false
    },
    "lmsId": {
      "type": "string"
    }
  }
}
